---
import tours from "../datas/tours.json"
import Tourpage from "../layouts/Tourpage__EN.astro";
import "../styles/style.css"
import * as Thing from "../Ititexts/goldenCircle/thingvellir1__EN.md"
import * as Geysir from "../Ititexts/goldenCircle/geysir1__EN.md"
import * as Gullfoss from "../Ititexts/goldenCircle/gullfoss1__EN.md"


const itiTextContent = [Thing.compiledContent(), Geysir.compiledContent(), Gullfoss.compiledContent()]

---

<Tourpage 
gallery={tours.GCE.__EN.body.gallery} 
pageTitle={tours.GCE.__EN.head.pageTitle} 
linkfr={tours.GCE.__EN.body.linkFr}
srcMobileHeader={tours.GCE.__EN.body.headerCover.srcMobile}
srcPCHeader={tours.GCE.__EN.body.headerCover.srcPC}
altHeader={tours.GCE.__EN.body.headerCover.alt}
titleHeader={tours.GCE.__EN.body.headerCover.title}

tourSeason={tours.GCE.__EN.body.season}
tourDuration={tours.GCE.__EN.body.duration}
tourDifficulty={tours.GCE.__EN.body.difficulty}
tourCapacity={tours.GCE.__EN.body.capacity}

ItiSectionTitle={tours.GCE.__EN.body.itiCover.itiTitle}
ItiSectionTitleSrcMobile={tours.GCE.__EN.body.itiCover.itiSrcMobile}
ItiSectionTitleSrcPC={tours.GCE.__EN.body.itiCover.itiSrcPC}
ItiSectionTitleAlt={tours.GCE.__EN.body.itiCover.itiAlt}
ItiSectionTitleTitle={tours.GCE.__EN.body.itiCover.itiTitle}
gtk={tours.GCE.__EN.body.goodToKnow}
wtb={tours.GCE.__EN.body.WhatToBring}
i={tours.GCE.__EN.body.Included}
ni={tours.GCE.__EN.body.nonIncluded}


AllItiItems={tours.GCE.__EN.body.itiItem}

itiTextContent={itiTextContent}









>


</Tourpage>
